
<div class="row">
   

    @foreach (var item in (IEnumerable<string>)ViewData["Conditions"])
    {

        if (item.ToString().Contains("Heading"))
        {
            <div class="card bg-light" style="margin-top:20px; text-align:center">
                <div class="card-body">
                    <h3>@item.ToString().Replace("Heading", "")</h3>
                </div>
            </div>
        }
        else if (item.ToString().Contains("FINALRULE"))
        { <h5 style="text-align: center">@item.ToString().Replace("FINALRULE", "")</h5>}
    else
    {
        <div class="form-group col-md-3">@item.ToString()</div>
    }

}
</div>
